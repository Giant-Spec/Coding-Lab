<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        var date = new Date()
        var month = date.getMonth() + 1;

        switch (month) {
            case 3:
            case 4:
            case 5:
                alert('봄입니다.');
                break;
            case 6:
            case 7:
            case 8:
                alert('여름입니다.');
                break;

            case 9:
            case 10:
            case 11:
                alert('가을입니다.');
                break;
            case 12:
            case 1:
            case 2:
                alert('겨울입니다.');
                break;
        }
    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        var rawInput = prompt('숫자를 입력해주세요', '숫자');
        var input = Number(rawInput);

        switch (input) {
            case 3:
            case 4:
            case 5:
                alert('봄입니다.');
                break;
            case 6:
            case 7:
            case 8:
                alert('여름입니다.');
                break;

            case 9:
            case 10:
            case 11:
                alert('가을입니다.');
                break;
            case 12:
            case 1:
            case 2:
                alert('겨울입니다.');
                break;
        }
    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

