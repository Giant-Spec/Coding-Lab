<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>

        parseInt('200가나다라') // Integer: 정수, -52, 0, 273
        parseFloat('52.273ㅁ오ㅓ로') // Float: 부동소수점 52.261

    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>

        // A를 출력해야겠다!
        alert('A');

        // 예약을 건다..!
        setTimeout(function () { alert('B'); }, 0);

        // C를 출력해야겠다!
        alert('C');

        // 무한 반복
        while (true) { }

        // 실행 끊김

    </script>
 
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        // let const var
        // var 전역 변수를 선언하는 키워드

        for (var i = 0; i < 3; i++) {
            //i = 0
            //예약을 걸고!
            //i = 1
            //예약을 걸고!
            //i = 2
            //예약을 걸고!
            //i = 3
            setTimeout(function () { alert(i); }, 0);
        }
        // i = 3
        //function () { alert(3); }
        //function () { alert(3); }
        //function () { alert(3); }
    </script>
 
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        // let const var
        // 스코프 내부에서만 작동하는 변수를 선언
        for (let i = 0; i < 3; i++) {
            //i = 0
            //예약을 걸고!
            //i = 1
            //예약을 걸고!
            //i = 2
            //예약을 걸고!
            //i = 3
            setTimeout(function () { alert(i); }, 0);
        }
    </script>
 
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        for (var i = 0; i < 3; i++) {
            (function (i) {
                setTimeout(function () {
                    alert(i);
                }, 0);
            })(i);
        }
    </script>
 
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        for (var i = 0; i < 3; i++) {
            (function (i) {
                var button = document.createElement("button");
                button.textContent = i;
                button.onclick = Function() {
                    alert(i);
                };
                document.body.appendChild(button);
            })(i);
        }
    </script>
 
</head>
<body>

</body>
</html>

