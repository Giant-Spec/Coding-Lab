<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        // 매개변수로 들어온 함수를 10번 실행해줌
        function callTenTimes(callback) {
            for (var i = 0; i < 10; i++) {
                callback();
            }
        }

        // 선언적 함수
        function testA() { alert("testA() 함수입니다"); }
        // 이름이 없는 함수 = 익명 함수
        var testB = ;

        // 함수를 호출합니다.
        callTenTimes(function () {
            alert("출력합니다");
        });
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        // 매개변수로 들어온 함수를 n번 실행해줌
        function callNTimes(n, callback) {
            for (var i = 0; i < n; i++) {
                callback();
            }
        }

        callNTimes(20, function () {
            alert("출력합니다");
        });
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        function returnFunction() {
            var output = function () {
                alert("Hello Function..!");
            };

            return output;
        }

        returnFunction()();
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        function returnFunction() {
            var a = 10;
            var b = 20;
            var c = 30;

            return function () {
                alert(a);
                alert(b);
                alert(c);
            };
        }

        returnFunction()();
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        function returnFunction(data) {
            var a = data * 1;
            var b = data * 2;
            var c = data * 3;

            return function () {
                alert(a);
                alert(b);
                alert(c);
            };
        }

        returnFunction(10)();
        returnFunction(20)();
        returnFunction(30)();
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        alert("시작되었습니다");

        //특정 시간 후에 함수를 1번만 실행하는 녀석
        setTimeout(function () {
            alert("1초가 지났습니다");
        }, 1000)

        alert
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        // alert("시작되었습니다");

        //특정 시간 후에 함수를 1번만 실행하는 녀석
        var id = setTimeout(function () {
            // alert("1초가 지났습니다");
        }, 1000);

        clearTimeout() // setTimeout()으로 설정한 타이머 제거

        alert
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        alert("시작되었습니다");

        // setInterval() 함수는 계속 반복
        var id = setInterval(function () {
            alert("1초가 지났습니다");
        }, 1000);

        clearInterval(id) // setTimeout()으로 설정한 타이머 제거

        alert
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        alert("시작되었습니다");

        // setInterval() 함수는 계속 반복
        var id = setInterval(function () {
            alert("1초가 지났습니다");
        }, 1000);

        setTimeout(function () {
            clearInterval(id);
        }, 3000)
    </script>
</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>

        var SECONDS = 1000;

        // setInterval() 함수는 계속 반복
        var id = setInterval(function () {
            alert("1초가 지났습니다");
        }, 1 * SECONDS);

        setTimeout(function () {
            clearInterval(id);
        }, 3 * SECONDS)

    </script>
</head>
<body>

</body>
</html>

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>

        // 인코딩: Encoding
        var a = escape('초코릿');
        // 디코딩: Decoding
        var b = unescape(a);

        alert(a);
        alert(b);

    </script>
</head>
<body>

</body>
</html>


