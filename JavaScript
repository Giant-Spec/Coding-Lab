    <meta charset="utf-8" />
    <title></title>
    <script>
        var rawInput = prompt('숫자를 입력해주세요', '숫자');
        if (rawInput == '') {
            rawInput = "dhjdhfjhf";
        }
        var input = Number(rawInput);

        switch (month) {
            case 3:
            case 4:
            case 5:
                alert('봄입니다.');
                break;
            case 6:
            case 7:
            case 8:
                alert('여름입니다.');
                break;

            case 9:
            case 10:
            case 11:
                alert('가을입니다.');
                break;
            case 12:
            case 1:
            case 2:
                alert('겨울입니다.');
                break;
        }
    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
    var input = prompt('숫자를 입력해주세요', '');
    var number = Number(input);

    (number > 0) ? alert('자연수입니다') : alert('자연수가 아닙니다');

    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        var input = prompt("입력해주세요", "");

        if (input.indexOf("안녕") >= 0) {
            alert("안녕하세요");
        } else if (input.indexOf("잘자") >= 0 || input.indexOf("잘 자") >= 0) {
            alert("안녕히 주무세요");
        }
    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        var rawInput = prompt("입력해주세요", "");
        if (rawInput == "") { rawInput = "ahdhd"; }
        var input = Number(rawInput);

        if (input % 4 == 0) {
            alert("4로 나눌 수 있는 숫자입니다.")
        } else {
            alert("4로 나눌 수 없는 숫자입니다.")
        }
    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        var string = "가나다라마바";
        string[0];
        string[1];
        string[2];
        string[0] = "하";
        string.length;

        alert(string);
        alert(string[0]);
        alert(string[1]);
        alert(string[2]);
        alert(string[3]);
        alert(string[4]);
        alert(string[5]);
        alert(string.length);
    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        var array = [10, "가나다", true, 40, 50, 60];
        alert(array.length); //6
        alert(array[array.length - 1]); // 마지막 번째 요소

        //배열 뒤에 요소를 추가
        array.push(70);
        array.push(80);
        array[array.length] = 90;
        alert(array);
    </script>

</head>
<body>

</body>
</html>


<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <script>
        var array = [10, "가나다", true, 40, 50, 60];
        alert(array.length); //6
        alert(array[array.length - 1]); // 마지막 번째 요소

        //배열 뒤에 요소를 추가
        array.push(70);
        array.push(80);
        array[array.length] = 90;
        alert(array);

        //배열의 요소를 제거
        array.splice(1, 1);
        alert(array);
    </script>

</head>
<body>

</body>
</html>


